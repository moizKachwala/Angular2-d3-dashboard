    "url": "git+https://github.com/moizKachwala/Angular2-d3-dashboard.git"
  },
  "keywords": [
    "Angular2",
    "D3",
    "Charts",
    "Dashboard"
  ],
  "author": "Moiz Kachwala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moizKachwala/Angular2-d3-dashboard/issues"
  },
  "homepage": "https://github.com/moizKachwala/Angular2-d3-dashboard#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
