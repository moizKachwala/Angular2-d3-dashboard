{
  "name": "angular-d3-dashboard",
  "version": "1.0.0",
  "description": "Angular2 application with D3 support",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
